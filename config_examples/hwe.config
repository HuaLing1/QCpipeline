# annotation
annot_scan_file illumina_scan_annot.RData

# vector of scanID to include in HWE
# controls and single PCA-defined ethnic groups only
scan_include_file ceu.RData

# scan-chromosome filter - optional, generally netCDF is filtered instead
#scan_chrom_filter jcmat.RData

annot_snp_file illumina_snp_annot.RData
annot_snp_missingCol missing.n1
annot_snp_rsIDCol rsID

# netCDF or GDS
geno_file illumina_geno_subj.nc
samp_geno_file illumina_geno.nc
samp_xy_file illumina_qxy.nc

# output
out_hwe_prefix hwe/hwe_ceu
out_qq_plot hwe/hwe_ceu_qq.png
out_inbrd_plot hwe/hwe_ceu_inbrd.pdf
out_maf_plot hwe/hwe_ceu_maf.png
out_clust_prefix hwe/hwe_ceu_cluster

# output - simulated results
out_sim_prefix		hwe/hwe_sim_ceu
# number of snps to simulate -- automatically picks the min of nsnp_simulate and non-monomorphic autosomal snps
nsnp_simulate		50000
