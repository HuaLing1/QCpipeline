# scan annotation
# should have columns "family", "father", "mother", "sex", "subj.plink"
annot_scan_file illumina_scan_annot.RData
annot_scan_subjectCol subjectID

# snp annotation
annot_snp_file illumina_snp_annot.RData
annot_snp_rsIDCol rsID
annot_snp_alleleACol alleleA
annot_snp_alleleBCol alleleB

# netCDF or GDS
# sample or subject level - will make plink file with only scans in subj.plink
#geno_file illumina_geno.nc
geno_file illumina_geno_subj.nc

# output
# prefix will be appended with .ped
out_plink_prefix plink/HapMap_TOP_subject_level
# prefix will be appended with .log
out_log_prefix plink/HapMap_plink
